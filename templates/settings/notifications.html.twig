{% extends 'settings/base.html.twig' %}

{% block title %}Notification Settings{% endblock %}

{% block body %}

    <div class='content'>
        {% block primary_nav %}
            {{ parent() }}
        {% endblock primary_nav %}

        {% block form %}
          {% for transport, form in tabbed_forms %}
            <div id="form_{{ transport }}" class='form'>
                {{ form(form) }}
            </div>
          {% endfor %}
        {% endblock form %}
    </div>

{% endblock body %}
