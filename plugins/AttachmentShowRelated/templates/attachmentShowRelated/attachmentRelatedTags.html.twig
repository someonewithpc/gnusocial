<section class="frame-section frame-section-padding">
    <div class="section-title">
        <span class="heading-no-margin">
            {{ 'Attachment tags' | trans }}
        </span>
    </div>

    {% for tag in related_tags %}
        <i>#{{ tag['tag'] }}</i>
    {% else %}
        {{ 'No tags' | trans }}
    {% endfor %}
</section>